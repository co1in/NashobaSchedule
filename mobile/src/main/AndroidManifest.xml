<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ctrmksw.nashobaschedule" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ctrmksw.nashobaschedule.CalendarActivity"
            android:label="Calendar"
            android:parentActivityName="com.ctrmksw.nashobaschedule.AgendaActivity" >
        </activity>
        <activity
            android:name="com.ctrmksw.nashobaschedule.AgendaActivity"
            android:label="Schedule" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ctrmksw.nashobaschedule.ConfigureScheduleActivity"
            android:label="@string/title_activity_configure_schedule">
        </activity>
    </application>

</manifest>
